#include <iostream>

using namespace std;

int main()
{
    int JumMhs;
    float Nilai;
    float JumNilai;
    float Mean;

    cout<<"Berapa Mahasiswanya? ";
    cin>>JumMhs;

    for(int i=0;i<JumMhs;i++)
    {
        cout<<"Berape Nilai lu? "
        cin>>Nilai;
        JumNilai=JumNilai+Nilai;
    }
}
